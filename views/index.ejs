
<%- include('layout') %>

<h2>Projects</h2>
<ul>
  <% projects.forEach(project => { %>
    <li>
      <h3><%= project.title %></h3>
      <p><%= project.description %></p>
      <img src="<%= project.imageUrl %>" alt="<%= project.title %>">
      <a href="/edit/<%= project._id %>">Edit</a>
      <form action="/delete/<%= project._id %>" method="POST">
        <button type="submit">Delete</button>
      </form>
    </li>
  <% }) %>
</ul>
